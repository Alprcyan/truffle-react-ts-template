{
  "name": "eth-toolbox-dapp",
  "version": "1.0.0",
  "description": "Ethereum Toolbox",
  "keywords": [
    "typescript",
    "react",
    "dapp"
  ],
  "bugs": {
    "url": "https://github.com/LibertyLocked/eth-toolbox-dapp/issues"
  },
  "author": "libertylocked",
